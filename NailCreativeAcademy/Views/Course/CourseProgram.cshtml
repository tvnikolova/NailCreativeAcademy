@model MyCourseModel

@{
    ViewData["Title"] = "Програма на курса";
}
<h2 class="text-center">@ViewData["Title"]</h2>
<hr />
<div class="text-center">
    <img src="~/image/Course/CourseProgram/@(Model.Program+".jpg")" class="card-img-top" alt="" />
    <a asp-controller="Course" asp-action="MyCourses" class="btn btn-secondary mb-2 w-100 p-3 fw-bold">Към моите курсове</a>

</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}